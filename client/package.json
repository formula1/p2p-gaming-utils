{
  "name": "p2p-algorithms",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "start:dev": "run-p start:dev:*",
    "start:dev:http": "http-server ./dev",
    "start-:dev:watch": "watch 'npm run dev:build' ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./dist && tsc",
    "dev:build": "run-s dev:build:*",
    "dev:build:ts": "browserify --full-paths -r ./src/index.tsx:init --debug -t hjsonify -t brfs -t [envlocalify --envfile=../shared/public.env ] -t browserify-css -p [ tsify -p ./tsconfig.json ] > ./dev/hidden.build.js && echo \"finished build\"",
    "dev:build:ugly": "terser ./dev/hidden.build.js -o ./dev/hidden.ugly.js --compress --mangle --sourcemap \"filename='./dev/hidden.ugly.min.js.map',url='hidden.ugly.min.js.map'\"",
    "dev:build:disc": "discify ./dev/hidden.build.js > ./dev/hidden.stats.html"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "promise-utils": "github:formula1/promise-utils",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.0",
    "@types/node": "^14.0.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "brfs": "^2.0.2",
    "browserify": "^16.5.0",
    "browserify-css": "^0.15.0",
    "chokidar-cli": "^2.0.0",
    "disc": "^1.3.3",
    "envify": "^4.1.0",
    "envlocalify": "^1.1.1",
    "foreach-cli": "^1.8.1",
    "hjsonify": "^0.2.0",
    "http-server": "^0.11.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "terser": "^4.3.8",
    "tinyify": "^2.5.1",
    "tsify": "^4.0.1",
    "typescript": "^3.6.2",
    "uglify-js": "^3.6.0",
    "watch": "^1.0.2"
  },
  "author": "",
  "license": "ISC"
}
